{
    "name": "picqer/exact-php-client",
    "type": "library",
    "description": "A PHP Client for the Exact API",
    "keywords": [
        "api",
        "php",
        "exact"
    ],
    "homepage": "http://github.com/picqer/exact-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Groen",
            "email": "info@picqer.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36"
    },
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "autoload": {
        "psr-4": {
            "Picqer\\": "src/Picqer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Picqer\\Tests\\": "tests/"
        }
    }
}
